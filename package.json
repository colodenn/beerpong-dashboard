    "@types/react-datepicker": "^4.3.4",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.1.1",
    "daisyui": "^1.21.0",
    "formidable": "^2.0.1",
    "framer-motion": "^6",
    "hamburger-react": "^2.4.1",
    "jimp": "^0.16.1",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dark-mode-toggle": "^0.2.0",
    "react-date-picker": "^8.3.6",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-grid-heatmap": "^1.1.0",
